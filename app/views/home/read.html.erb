<div class = "container" style="margin-top:50px">
    <a href="/home/index">글쓰러가기</a>
    <p></p>
    <% @post.each do |p| %>
    <div class="card">
      <div class="card-header">
        <%= p.title %>
      </div>
      <div class="card-block">
        <blockquote class="card-blockquote">
            <p><%= p.content %></p>
            <footer><a href="/home/delete/<%=p.id %>" style="float:right"> 글 삭제</a> <a href="/home/update/<%=p.id %>" style="float:right">글 수정 /</a></footer>
        </blockquote>
      </div>
    </div>
     <p></p>
    <% end %>
</div>

